{
  "name": "react-template",
  "version": "0.3.8",
  "description": "React and typescript template",
  "main": "src/index.tsx",
  "type": "module",
  "author": "Alexey-Koran <alexey.koran@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexey-koran/react-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-koran/react-template.git"
  },
  "keywords": [
    "react",
    "typescript",
    "react template",
    "react boilerplate"
  ],
  "homepage": "https://github.com/alexey-koran/react-template#readme",
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=22.19.0"
  },
  "scripts": {
    "dev": "node --import ./src/ts-node.register.mjs node_modules/webpack/bin/webpack serve --color --hot --mode=development",
    "prod": "node --import ./src/ts-node.register.mjs node_modules/webpack/bin/webpack serve --color --hot --mode=production",
    "buildapp": "node --import ./src/ts-node.register.mjs node_modules/webpack/bin/webpack --mode=production",
    "clean": "rm -f -r build",
    "build": "pnpm clean && pnpm buildapp",
    "analyze": "node --import ./src/ts-node.register.mjs node_modules/webpack/bin/webpack --analyze --mode=production",
    "prepare": "husky && echo 'pnpm lint-staged' > .husky/pre-commit",
    "prettier": "prettier --check \"**/*.{js,cjs,mjs,jsx,cjsx,mjsx,ts,cts,mts,tsx,ctsx,mtsx,pcss}\"",
    "prettier:fix": "prettier --write \"**/*.{js,cjs,mjs,jsx,cjsx,mjsx,ts,cts,mts,tsx,ctsx,mtsx,pcss}\"",
    "lint": "eslint --concurrency=auto **/*.{js,cjs,mjs,jsx,cjsx,mjsx,ts,cts,mts,tsx,ctsx,mtsx}",
    "lint:fix": "eslint --concurrency=auto **/*.{js,cjs,mjs,jsx,cjsx,mjsx,ts,cts,mts,tsx,ctsx,mtsx} --fix",
    "lint:fix-staged": "eslint --concurrency=auto --fix",
    "lint:css": "stylelint src",
    "lint:css:fix": "pnpm lint:css -- --fix",
    "lint:css:fix-staged": "pnpm stylelint -- --fix",
    "lint-all": "pnpm prettier && pnpm lint && pnpm lint:css",
    "fix-all": "pnpm prettier:fix && pnpm lint:fix && pnpm lint:css:fix",
    "fix-staged": "lint-staged",
    "test:eslint-config": "eslint-config-inspector",
    "test:webpack-config": "node --import ./src/ts-node.register.mjs node_modules/webpack/bin/webpack configtest",
    "svgo": "cross-env svgo $FILE",
    "svgo-all": "svgo -r -f src"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@alexey-koran/eslint-config": "^0.3.0",
    "@alexey-koran/prettier-config": "^0.1.3",
    "@alexey-koran/tsconfig": "^0.1.0",
    "@eslint/config-inspector": "^1.2.0",
    "@eslint/js": "^9.34.0",
    "@svgr/webpack": "^8.1.0",
    "@types/dotenv-webpack": "^7.0.8",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "clsx": "^2.1.1",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.1",
    "esbuild-loader": "^4.3.0",
    "eslint": "^9.34.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-assets": "^6.0.0",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.3.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-error-boundary": "^4.0.13",
    "style-loader": "^4.0.0",
    "stylelint": "^16.23.1",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-prettier": "^5.0.3",
    "svgo": "4.0.0",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild"
    ]
  }
}
